    "formik": "^2.2.9",
    "mongodb": "^4.9.0",
    "next": "12.0.0",
    "next-auth": "^3.29.0",
    "nodemon": "^2.0.13",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-material-ui-carousel": "^3.4.2",
    "react-window": "^1.8.6",
    "styled-components": "^5.3.1",
